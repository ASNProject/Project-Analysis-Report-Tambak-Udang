<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorBackgroung</item>
        <item name="android:windowBackground">@color/colorBackgroung</item>
        <item name="android:itemBackground">@color/colorBackgroung</item>
        <item name="android:textColorPrimary">@color/colorBackgroung</item>

    </style>

    <style name="TambahTambak" parent="Theme.AppCompat.Dialog.Alert">
        <item name="colorAccent" type="">@color/colorDarkBlue</item>
        <item name="android:textColorPrimary">@color/colorDarkBlue</item>
        <item name="android:windowBackground">@color/colorBackgroung</item>
    </style>

    <style name="AppBaseTheme" parent="AppTheme">

    </style>

    <style name="AppThemes" parent="AppBaseTheme">

    </style>

</resources>
